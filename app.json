{
  "expo": {
    "name": "TEMPUS",
    "slug": "tempus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/tempus-logo.png",
    "splash": {
      "image": "./src/assets/tempus-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
