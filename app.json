        "foregroundImage": "./src/assets/tempus-logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.noorulislam.tempus",
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./src/assets/tempus-logo.png"
    },
  "owner": "noorulislam"
  }
}
